    "helmet": "^3.12.1",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
